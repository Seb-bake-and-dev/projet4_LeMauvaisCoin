{{ form_start(form) }}
    <div class="form-group offset-2 col-8">
        {{ form_row(form.lastName) }}
    </div>
    <div class="form-group offset-2 col-8">
        {{ form_row(form.firstName) }}
    </div>
    <div class="form-group offset-2 col-8">
        {{ form_row(form.email) }}
    </div>
    <div class="form-group offset-2 col-8">
        {{ form_row(form.password) }}
    </div>
    <div class="form-group offset-2 col-8">
        {{ form_row(form.city) }}
    </div>
    <button class="btn btn-primary">{{ button_label|default('Valider') }}</button>
{{ form_end(form) }}